
<section class="message">
<div class="messages">
  <h1 class="message_title">Message preview</h1>
  <p>
    <strong>Email:</strong>
    <%= @message.email %>
  </p>

  <p>
    <strong class="">Messages:</strong>
    <%= @message.messages %>
  </p>

  <%= link_to 'Edit', edit_message_path(@message), class:"msg" %> |
  <%= link_to 'Back', '/', class:"msg" %>
</div>
</section>